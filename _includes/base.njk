<!doctype html>
<html lang="{{ metadata.language }}">
    <head>{% include "partials/seo.njk" %}
    <link href="https://fonts.googleapis.com/css2?family=Cutive+Mono&display=swap" rel="stylesheet">
    <body>{% include "partials/style.njk" %}<a href="#main-content" aria-label="Read Content Article" class="skip-to-content">Skip to content</a>
        <header>
            <nav>
                <ul>{% for navs in metadata.navbar %}<li>
                    <a href="{{navs.url}}" aria-label="Navigation Menu for {{navs.nav}}">{{navs.nav}}</a>
                </li>{% endfor %}</ul>
        </nav>
    </header>
    <button aria-label="dark light mode" id="mode-toggle">
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-lightning-charge-fill" viewBox="0 0 16 16">
            <path d="M11.251.068a.5.5 0 0 1 .227.58L9.677 6.5H13a.5.5 0 0 1 .364.843l-8 8.5a.5.5 0 0 1-.842-.49L6.323 9.5H3a.5.5 0 0 1-.364-.843l8-8.5a.5.5 0 0 1 .615-.09z"/>
            <title>Dark Light Mode</title>
        </svg>
    </button>
    <main aria-label="Content Article Main" id="main-content">
        {{ content | safe }}
    </main>
    <footer>
        <p>
<a aria-label="Developer for this website" href="{{metadata.footer.text1_url}}" target="_blank">{{metadata.footer.text1}}</a>
        </p>
    </footer>{%- js %}{% include "public/js/brutal.js" %}{% endjs %}<script type="module" src="{% getBundleFileUrl "js" %}"></script>
</body>
</html>
